CREATE MEMORY TABLE "PUBLIC"."BILLING"(
    "ID" INTEGER GENERATED BY DEFAULT AS IDENTITY(START WITH 1) NOT NULL,
    "DATES" CHARACTER VARYING(255) DEFAULT NULL,
    "CLIENTS" CHARACTER VARYING(255) DEFAULT NULL,
    "PROJECT" CHARACTER VARYING(255) DEFAULT NULL,
    "PROJECT_CODE" CHARACTER VARYING(255) DEFAULT NULL,
    "HOURS" DOUBLE PRECISION DEFAULT NULL,
    "BILLABLE" CHARACTER VARYING(5) DEFAULT NULL,
    "FIRST_NAME" CHARACTER VARYING(255) DEFAULT NULL,
    "LAST_NAME" CHARACTER VARYING(255) DEFAULT NULL,
    "BILL_RATE" INTEGER DEFAULT NULL
);   
ALTER TABLE "PUBLIC"."BILLING" ADD CONSTRAINT "PUBLIC"."CONSTRAINT_2" PRIMARY KEY("ID");      
